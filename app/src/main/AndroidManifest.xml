<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zooapp.jmdel.fr.zooapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.animal.AnimalActivity"
            android:label="@string/title_activity_animal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.enclos.EnclosActivity"
            android:label="@string/title_activity_enclos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.foods.FoodActivity"
            android:label="@string/title_activity_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.foods.FoodDetailActivity"
            android:label="@string/title_activity_food_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.animal.AnimalDetailActivity" />
        <activity
            android:name=".activity.foods.FoodFormActivity"
            android:label="@string/title_activity_food_form"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.foods.FoodUpdateActivity"></activity>
    </application>

</manifest>