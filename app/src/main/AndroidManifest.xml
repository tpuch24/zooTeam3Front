<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zooapp.jmdel.fr.zooapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.animal.AnimalActivity"
            android:label="@string/title_activity_animal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.enclos.EnclosActivity"
            android:label="@string/title_activity_enclos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.food.FoodActivity"
            android:label="@string/title_activity_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.food.FoodDetailActivity"
            android:label="@string/title_activity_food_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.animal.AnimalDetailActivity" />
        <activity
            android:name=".activity.food.FoodFormActivity"
            android:label="@string/title_activity_food_form"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.food.FoodUpdateActivity"></activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>